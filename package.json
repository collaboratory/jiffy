    "@babel/plugin-transform-runtime": "^7.4.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@emitterware/emitter": "0.0.1",
    "axios": "^0.18.0",
    "object-hash": "^1.3.1"
