{
  "name": "@collaboratory/jiffy",
  "version": "0.0.1",
  "description": "Fast, simple, composable async JSX toolkit",
  "main": "build/jiffy.js",
  "files": [
    "build/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "example": "npx parcel example/index.html -d example/dist -p 3000",
    "example-react": "npx parcel example-react/index.html -d example-react/dist -p 3001",
